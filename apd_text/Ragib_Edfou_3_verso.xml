<lb n="2"/>  شهد علي بن محمّد بن يعقوب <gap reason="illegible" precision="low" extent="3"/> 
<lb n="3"/>  <unclear reason="undefined" cert="high">أحمد</unclear> <unclear reason="undefined" cert="high">بدنو</unclear> سنة <unclear reason="undefined" cert="high">ثمانية</unclear> <gap reason="illegible" precision="low" extent="3"/> 
<lb n="4"/>  وثلثمائة وكتب شهادته عنــ<supplied reason="lost" cert="high">ـه</supplied> <gap reason="illegible" precision="low" extent="3"/> 
<lb n="5"/>  شهد محمّد بن <gap reason="illegible" precision="low" extent="3"/> 
<lb n="6"/>  وثلثمائة على ذلك وأحمـ<supplied reason="lost" cert="high">ـد</supplied> <gap reason="illegible" precision="low" extent="3"/> 
<lb n="7"/>  شهد عبد اللّه بن أحمـ<supplied reason="lost" cert="high">ـد</supplied> <gap reason="illegible" precision="low" extent="3"/> 
